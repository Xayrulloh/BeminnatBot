    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx}\""
  },
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@9.12.2",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.717.0",
    "@grammyjs/auto-retry": "1.1.1",
    "axios": "1.5.0",
    "discord.js": "14.13.0",
    "dotenv": "^16.4.7",
    "fastify": "4.23.2",
    "fuzzy": "0.1.3",
    "grammy": "1.19.0",
    "grammy-scenes": "10.2.0",
    "mongoose": "7.5.2",
    "ts-node": "^10.9.2",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@grammyjs/types": "3.3.0",
    "@types/node": "20.6.4",
    "prettier": "3.0.3",
    "rimraf": "5.0.1",
